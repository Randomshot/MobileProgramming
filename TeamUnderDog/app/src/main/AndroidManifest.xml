<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamunderdog">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".exerciselist.ExerciseAddActivity"
            android:label="@string/title_activity_exercise_add">
        </activity>
        <activity android:name="com.example.exercisetips.TipsViewPageActivity"/>
        <activity android:name="com.example.exercisetips.TipsMainActivity"/>

        <activity android:name=".routine.SportsActivity" />
        <activity android:name=".routine.RoutineActivity"/>

        <activity android:name=".exerciselist.ExerciseListActivity" />

        <activity android:name=".record.ExerciseRecordActivity"/>
        <activity android:name=".record.ExerciseTimerActivity"/>
        <activity android:name=".record.AddPhysicalRecordActivity"/>
        <activity android:name=".record.ShowPhysicalRecordActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>



</manifest>